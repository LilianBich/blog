<%= simple_form_for(@article, html: {multipart: true}) do |f| %>
  <%= f.error_notification %>

    <%= f.input :title, label: false, placeholder: "title" %>
    <%= f.input :body %>
    <%= f.input :published %>
    <%= f.input :country %>
    <%= f.input :picture %>

    <%= f.button :submit %>
<% end %>
